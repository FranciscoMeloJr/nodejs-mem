{
    "name": "node-memstats",
    "version": "1.0.0",
    "description": "A simple Node.js service to track memory usage",
    "main": "server.js",
    "scripts": {
      "start": "node server.js",
      "openshift": "nodeshift --expose",
      "openshift:clean": "nodeshift undeploy --removeAll",
      "openshift:dockerbuild": "nodeshift --build.strategy Docker --expose",
      "openshift:dockerbuild:clean": "nodeshift undeploy --removeAll --build.strategy Docker"
    },
    "dependencies": {
      "express": "^4.18.2",
      "nodeshift": "^12.1.0"
    },
    "engines": {
      "node": ">=18"
    }
  }